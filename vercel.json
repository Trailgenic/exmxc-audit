{
  "version": 2,
  "builds": [
    { "src": "api/audit.js", "use": "@vercel/node" },
    { "src": "api/predictive-audit.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "predictive.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/api/audit$", "dest": "api/audit.js" },
    { "src": "^/api/predictive-audit$", "dest": "api/predictive-audit.js" },
    { "src": "^/predictive-audit$", "dest": "/predictive.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
