{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "api/audit.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/public/index.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
